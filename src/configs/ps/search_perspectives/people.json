{
    "id": "people",
    "endpoint": {
        "url": "https://ldf.fi/semparl/sparql",
        "useAuth": true,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesPeople.js",
    "baseURI": "http://ldf.fi/semparl",
    "URITemplate": "<BASE_URI>/people/<LOCAL_ID>",
    "facetClass": "bioc:Person",
    "langTag": "fi",
    "frontPageImage": "main_page/henriksson2.jpg",
    "searchMode": "faceted-search",
    "defaultActiveFacets": [],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "maps": {
        "peoplePlaces": {
            "center": [
                60.43,
                20.37
            ],
            "zoom": 3
        },
        "peopleMigrations": {
            "center": [
                60.43,
                20.37
            ],
            "zoom": 3
        },
        "personInstanceMap": {
            "center": [
                60.43,
                20.37
            ],
            "zoom": 3
        }
    },
    "resultClasses": {
        "people": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "personPropertiesFacetResults",
                "pagesize": 10,
                "paginatedResultsAlwaysExpandRows": true,
                "paginatedResultsRowContentMaxHeight": 150,
                "sortBy": "prefLabel",
                "sortDirection": "asc"
            },
            "instanceConfig": {
                "propertiesQueryBlock": "personPropertiesInstancePage",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    },
                    "personInstancePageEvents": {
                        "tabID": 1,
                        "tabPath": "events",
                        "tabIcon": "CalendarToday",
                        "component": "InstancePageTable",
                        "fetchResultsWhenMounted": true,
                        "sparqlQuery": "personEventsQuery",
                        "properties": [
                            {
                                "id": "prefLabel",
                                "valueType": "object",
                                "makeLink": false,
                                "externalLink": false,
                                "sortValues": false,
                                "numberedList": false
                            },
                            {
                                "id": "event",
                                "valueType": "object",
                                "makeLink": false,
                                "externalLink": false,
                                "sortValues": false,
                                "numberedList": true,
                                "expandedOnInstancePage": true
                            }
                        ]
                    },
                    "personInstancePageSpeeches": {
                        "tabID": 2,
                        "tabPath": "speeches",
                        "tabIcon": "KeyboardVoice",
                        "component": "InstancePageTable",
                        "fetchResultsWhenMounted": true,
                        "sparqlQuery": "personSpeechesQuery",
                        "properties": [
                            {
                                "id": "prefLabel",
                                "valueType": "object",
                                "makeLink": false,
                                "externalLink": false,
                                "sortValues": false,
                                "numberedList": false
                            },
                            {
                                "id": "event",
                                "valueType": "object",
                                "makeLink": false,
                                "externalLink": false,
                                "sortValues": false,
                                "numberedList": true,
                                "expandedOnInstancePage": true
                            }
                        ]
                    },
                    "personInstanceMap": {
                        "tabID": 3,
                        "tabPath": "map",
                        "tabIcon": "LocationOn",
                        "component": "LeafletMap",
                        "pageType": "instancePage",
                        "mapMode": "marker",
                        "showInstanceCountInClusters": false,
                        "showExternalLayers": true,
                        "customMapControl": true,
                        "sparqlQuery": "placeMapQuery",
                        "instanceConfig": {
                            "createPopUpContent": "createPopUpContentAs"
                        }
                    },
                    "personInstancePageNetwork": {
                        "tabID": 4,
                        "tabPath": "network",
                        "tabIcon": "BubbleChart",
                        "component": "Network",
                        "pageType": "instancePage",
                        "facetClass": "people",
                        "sparqlQuery": "networkLinkQuery",
                        "sparqlQueryNodes": "networkNodeQuery",
                        "useNetworkAPI": true,
                        "limit": 50,
                        "optimize": 1.0,                  
                        "preprocess": "preprocessParliamentSampoPeopleNetwork",                      
                        "style": [
                            {
                                "selector": "node",
                                "style": {
                                    "shape": "ellipse",
                                    "font-size": "data(font_size)",
                                    "background-color": "data(color)",
                                    "label": "data(prefLabel)",
                                    "height": "data(size)",
                                    "width": "data(size)",
                                    "border-width": "data(border_width)",
                                    "border-color": "black"
                                }
                            },
                            {
                                "selector": "edge",
                                "style": {
                                    "width": "data(weight)",
                                    "line-color": "data(color)",
                                    "curve-style": "bezier",
                                    "content": "data(prefLabel)",
                                    "color": "hsl(30, 64%, 35%)",
                                    "opacity": 0.6,
                                    "target-arrow-shape": "triangle",
                                    "target-arrow-color": "data(color)",
                                    "font-size": "8",
                                    "text-valign": "center",
                                    "edge-text-rotation": "autorotate",
                                    "text-background-opacity": 1,
                                    "text-background-color": "white",
                                    "text-background-shape": "roundrectangle"
                                }
                            }
                        ],
                        "layout": {
                            "name": "cose",
                            "idealEdgeLength": 100,
                            "nodeOverlap": 20,
                            "refresh": 20,
                            "fit": true,
                            "padding": 30,
                            "randomize": false,
                            "componentSpacing": 100,
                            "nodeRepulsion": 400000,
                            "edgeElasticity": 100,
                            "nestingFactor": 5,
                            "gravity": 80,
                            "numIter": 1347,
                            "initialTemp": 200,
                            "coolingFactor": 0.95,
                            "minTemp": 1
                        }    
                    }
                }
            }
        },
        "personTimeSeries": {
            "tabID": 1,
            "component": "ApexCharts",
            "createChartData": "createMultipleLineChartData",
            "stroke": {
                "curve": "straight",
                "width": 2
            },
            "title": "Syntyneet, kuolleet ja kautensa aloittaneet edustajat",
            "xaxisTitle": "Vuosi",
            "xaxisType": "category",
            "xaxisTickAmount": 20,
            "yaxisTitle": "Henkilöiden lukumäärä",
            "tabPath": "timeseries",
            "tabIcon": "ShowChart",
            "sparqlQuery": "timeSeriesQuery",
            "facetClass": "people",
            "filterTarget": "person__id",
            "resultMapper": "mapMultipleLineChart",
            "resultMapperConfig": {
                "fillEmptyValues": true
            }
        },
        "personAges": {
            "tabID": 2,
            "component": "ApexCharts",
            "createChartData": "createMultipleLineChartData",
            "stroke": {
                "curve": "straight",
                "width": 2
            },
            "title": "Edustajien iät kauden alkaessa ja loppuessa",
            "xaxisTitle": "Ikä",
            "xaxisType": "category",
            "xaxisTickAmount": 20,
            "yaxisTitle": "Henkilöiden lukumäärä",
            "tabPath": "by_age",
            "tabIcon": "ShowChart",
            "sparqlQuery": "ageQuery",
            "facetClass": "people",
            "filterTarget": "person__id",
            "resultMapper": "mapMultipleLineChart",
            "resultMapperConfig": {
                "fillEmptyValues": true
            }
        },
        "peopleMigrations": {
            "tabID": 3,
            "component": "Deck",
            "tabPath": "migrations",
            "tabIcon": "Redo",
            "sparqlQuery": "peopleMigrationsQuery",
            "facetClass": "people",
            "filterTarget": "person",
            "layerType": "arcLayer",
            "arcWidthVariable": "instanceCountScaled",
            "instanceVariable": "person",
            "showTooltips": true,
            "postprocess": {
                "func": "linearScale",
                "config": {
                    "variable": "instanceCount",
                    "minAllowed": 3,
                    "maxAllowed": 30
                }
            }
        },
        "peopleMigrationsDialog": {
            "sparqlQuery": "peopleMigrationsDialogQuery",
            "filterTarget": "id"
        },
        "peoplePlaces": {
            "tabID": 4,
            "component": "LeafletMap",
            "tabPath": "map",
            "tabIcon": "AddLocation",
            "sparqlQuery": "peopleEventPlacesQuery",
            "facetClass": "people",
            "filterTarget": "person",
            "resultMapper": "mapPlaces",
            "instanceConfig": {
                "propertiesQueryBlock": "placePropertiesInfoWindow",
                "relatedInstances": "peopleRelatedTo",
                "createPopUpContent": "createPopUpContentAs"
            }
        }
    },
